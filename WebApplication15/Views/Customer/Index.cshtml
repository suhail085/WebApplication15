@model IEnumerable<WebApplication15.Models.Location>
@{
}
<table class="table table-bordered">
    <tr>
        <th>Location </th>
        <th>Detail</th>
    </tr>
    @foreach(var location in Model)
            {
                <tr>
                    <td>
                       <a asp-action="CustomerList" asp-controller="Customer" asp-route-id="location.Id"> @location.Place</a>
                    </td>
                    <td>@location.Detail</td>
                </tr>
            }
</table>